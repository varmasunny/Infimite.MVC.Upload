@model IEnumerable<ImageViewModel>

@{
    ViewData["Title"] = "Index";
}


<div class="row">
    <div class="col-md-7">
        <div class="row">
            <div class="col">
                <h2>List of Images</h2>
            </div>
            <div class="col text-end">
                <a asp-controller="Image" asp-action="Create" class="btn btn-warning">Upload Image</a>
            </div>
        </div>
        <div class="row">
            <table>
                <thead>
                    <tr>
                        <th>Image Name</th>&nbsp;
                        &nbsp;
                        &nbsp;
                        
                        <th>Image Url</th>
                      
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>@item.ImageName</td>
                            <td>@item.ImageUrl</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>